<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="460.0" prefWidth="657.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="killer_app.ChatController">
   <children>
      <TableView fx:id="table" layoutX="12.0" layoutY="14.0" prefHeight="334.0" prefWidth="577.0">
        <columns>
          <TableColumn fx:id="sender" prefWidth="127.0" text="Sender" />
          <TableColumn fx:id="text" prefWidth="447.0" text="Text" />
        </columns>
      </TableView>
      <Text layoutX="27.0" layoutY="373.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text:" />
      <TextField fx:id="textmessage" layoutX="69.0" layoutY="352.0" />
      <Button layoutX="264.0" layoutY="352.0" mnemonicParsing="false" onAction="#SendMessage" text="Send" />
      <ComboBox fx:id="friends" layoutX="14.0" layoutY="415.0" prefWidth="150.0" />
      <Button layoutX="176.0" layoutY="415.0" mnemonicParsing="false" onAction="#AddUser" text="Add" />
      <Button layoutX="589.0" layoutY="415.0" mnemonicParsing="false" onAction="#Back" text="Back" />
      <Button layoutX="503.0" layoutY="352.0" mnemonicParsing="false" onAction="#RemoveUser" text="Leave chat" />
   </children>
</AnchorPane>
