          <TableColumn fx:id="status" minWidth="0.0" prefWidth="100.0" text="Status" />
        </columns>
      </TableView>
      <TextField fx:id="friendname" layoutX="229.0" layoutY="353.0" />
      <Text layoutX="121.0" layoutY="374.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name of friend:" />
      <Button layoutX="423.0" layoutY="353.0" mnemonicParsing="false" onAction="#SendFriendRequest" text="Send friendrequest" />
      <Button layoutX="444.0" layoutY="170.0" mnemonicParsing="false" onAction="#RemoveFriend" text="Remove friend" />
      <Button layoutX="14.0" layoutY="353.0" mnemonicParsing="false" onAction="#Back" text="Back" />
      <Button layoutX="290.0" layoutY="224.0" mnemonicParsing="false" onAction="#CreateGroupChat" text="Create" />
      <TextField fx:id="groupname" layoutX="96.0" layoutY="224.0" />
      <Text layoutX="10.0" layoutY="245.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Group name:" />
